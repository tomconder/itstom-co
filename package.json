{
  "name": "itstom-co",
  "description": "It's Tom",
  "version": "1.0.4",
  "author": "Tom Conder <tomc@itstom.co>",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tomconder/itstom-co"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production gatsby build",
    "clean": "rimraf .cache public coverage",
    "develop": "gatsby develop",
    "deploy": "node scripts/deploy",
    "lint": "eslint --ignore-path .gitignore -c .eslintrc.json --ext js,jsx .",
    "lint:fix": "eslint --ignore-path .gitignore -c .eslintrc.json --ext js,jsx --fix .",
    "lint:sass": "sass-lint --config sass-lint.yml --verbose --no-exit 'src/**/**.scss'",
    "serve": "gatsby serve",
    "test": "cross-env NODE_ENV=test jest --no-cache",
    "test:coverage": "cross-env NODE_ENV=test jest --no-cache --coverage"
  },
  "dependencies": {
    "@reach/router": "^1.1.1",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "gatsby": "^2.17.4",
    "gatsby-image": "^2.2.29",
    "gatsby-link": "^2.2.22",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-gtag": "^1.0.3",
    "gatsby-plugin-html-attributes": "^1.0.5",
    "gatsby-plugin-manifest": "^2.2.23",
    "gatsby-plugin-nprogress": "^2.1.12",
    "gatsby-plugin-offline": "^3.0.16",
    "gatsby-plugin-purgecss": "^4.0.0",
    "gatsby-plugin-react-head": "^1.1.0",
    "gatsby-plugin-sass": "^2.1.20",
    "gatsby-plugin-sharp": "^2.2.32",
    "gatsby-plugin-styled-components": "^3.1.11",
    "gatsby-source-filesystem": "^2.1.33",
    "gatsby-transformer-sharp": "^2.3.0",
    "prop-types": "^15.6.2",
    "raf": "^3.4.1",
    "react": "^16.11.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dom": "^16.11.0",
    "react-feather": "^2.0.3",
    "react-head": "^3.1.1",
    "react-test-renderer": "^16.11.0",
    "sftp-sync-deploy": "^0.7.1",
    "styled-components": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-styled-components": "^1.8.0",
    "chance": "^1.1.3",
    "cross-env": "^6.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "gatsby-plugin-sitemap": "^2.2.19",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "jest-axe": "^3.1.1",
    "jest-enzyme": "^7.0.0",
    "node-sass": "^4.13.0",
    "rimraf": "^3.0.0",
    "sass-lint": "^1.12.1",
    "webpack": "^4.41.2"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js"
    },
    "testRegex": "/.*(__tests__\\/.*)|(.*(test|spec))\\.jsx?$",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "coverageDirectory": "coverage",
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/tests/setupTests.js",
      "<rootDir>/loadershim.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
